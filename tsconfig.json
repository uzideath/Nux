{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"types": ["node"],
		"moduleResolution": "Node",
		"baseUrl": "./src",
		"paths": {
			"#lib/*": ["lib/*"],
			"#core": ["core/index.ts"],
			"#root/*": ["*"]
		},
		"rootDir": ".",
		"outDir": "dist",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"noUnusedLocals": false,
		"skipLibCheck": true
	}
}
